<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".add_ambulance">

    <EditText
        android:id="@+id/driver_name_id"
        android:layout_width="238dp"

        android:layout_height="56dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/phone_id"
        android:layout_width="238dp"

        android:layout_height="56dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="292dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/district_id"
        android:layout_width="238dp"

        android:layout_height="56dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="424dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/save_id"
        android:layout_width="175dp"
        android:layout_height="54dp"
        android:layout_marginStart="154dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="83dp"
        android:text="SAVE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/district_id" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="117dp"
        android:layout_height="49dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="146dp"
        android:layout_marginEnd="23dp"
        android:text="Name"
        app:layout_constraintEnd_toStartOf="@+id/driver_name_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="117dp"
        android:layout_height="49dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="23dp"
        android:text="Phone"
        app:layout_constraintEnd_toStartOf="@+id/driver_name_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="117dp"
        android:layout_height="49dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="424dp"
        android:layout_marginEnd="23dp"
        android:text="District"
        app:layout_constraintEnd_toStartOf="@+id/driver_name_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>